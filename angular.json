{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "modules",
  "projects": {
    "ng-zawgyi-detector-demo": {
      "root": "",
      "sourceRoot": "samples/ng-zawgyi-detector-demo/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ng-zawgyi-detector-demo",
            "index": "samples/ng-zawgyi-detector-demo/src/index.html",
            "main": "samples/ng-zawgyi-detector-demo/src/main.ts",
            "polyfills": "samples/ng-zawgyi-detector-demo/src/polyfills.ts",
            "tsConfig": "samples/ng-zawgyi-detector-demo/tsconfig.app.json",
            "assets": [
              {
                "input": "modules/ng-zawgyi-detector/rules/",
                "glob": "**/*.json",
                "output": "./assets/zawgyi-detect-rules"
              },
              "samples/ng-zawgyi-detector-demo/src/robots.txt",
              "samples/ng-zawgyi-detector-demo/src/favicon.ico",
              "samples/ng-zawgyi-detector-demo/src/assets"
            ],
            "styles": [
              "samples/ng-zawgyi-detector-demo/src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "node_modules/normalize.css"
              ]
            },
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "samples/ng-zawgyi-detector-demo/src/environments/environment.ts",
                  "with": "samples/ng-zawgyi-detector-demo/src/environments/environment.prod.ts"
                }
              ],
              "tsConfig": "samples/ng-zawgyi-detector-demo/tsconfig.app.prod.json",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-zawgyi-detector-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-zawgyi-detector-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-zawgyi-detector-demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "samples/ng-zawgyi-detector-demo/src/test.ts",
            "polyfills": "samples/ng-zawgyi-detector-demo/src/polyfills.ts",
            "tsConfig": "samples/ng-zawgyi-detector-demo/tsconfig.spec.json",
            "karmaConfig": "samples/ng-zawgyi-detector-demo/karma.conf.js",
            "styles": [
              "samples/ng-zawgyi-detector-demo/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              {
                "input": "modules/ng-zawgyi-detector/rules/",
                "glob": "**/*.json",
                "output": "./assets/zawgyi-detect-rules"
              },
              "samples/ng-zawgyi-detector-demo/src/favicon.ico",
              "samples/ng-zawgyi-detector-demo/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "samples/ng-zawgyi-detector-demo/tsconfig.app.json",
              "samples/ng-zawgyi-detector-demo/tsconfig.spec.json",
              "samples/ng-zawgyi-detector-demo/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "samples/ng-zawgyi-detector-demo/e2e/protractor.conf.js",
            "devServerTarget": "ng-zawgyi-detector-demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-zawgyi-detector-demo:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "ng-zawgyi-detector-demo"
}
